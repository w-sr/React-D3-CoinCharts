{
  "name": "coinbase-charts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "currency-formatter": "^1.2.1",
    "d3-array": "^1.2.0",
    "d3-ease": "^1.0.3",
    "d3-interpolate-path": "^2.0.0",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-time-format": "^2.0.5",
    "d3-transition": "^1.1.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "serve": "^6.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm build/static/**/*.map",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint --color -- \"src/**/*.js\"",
    "stylelint": "stylelint --color -- \"src/**/*.css\"",
    "check": "npm run stylelint && npm run eslint",
    "updatePriceData": "scripts/updatePriceData.sh ./public/priceData",
    "deploy": "npm run check && npm run build && cp now.json ./build && now ./build",
    "now-start": "serve ./build"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0"
  }
}
